	<select id="selectListVO"
		resultType="com.entity.vo.QicherukuVO" >
		SELECT * FROM qicheruku  qicheruku         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.QicherukuVO" >
		SELECT  qicheruku.* FROM qicheruku  qicheruku 	
 		<where> 1=1 ${ew.sqlSegment}</where>
